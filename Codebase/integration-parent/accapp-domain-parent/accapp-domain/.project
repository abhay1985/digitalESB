<projectDescription>
  <name>acc-simple-domain</name>
  <comment>Mule domain projects that has shared resources and configuration 
which is nNon JTA aware. All mule apps and flows that need Non JTA based resources 
should be kept in this domain.</comment>
  <projects/>
  <buildSpec>
    <buildCommand>
      <name>org.eclipse.jdt.core.javabuilder</name>
    </buildCommand>
    <buildCommand>
      <name>org.mule.tooling.core.muleDomainBuilder</name>
    </buildCommand>
    <buildCommand>
      <name>org.mule.tooling.core.muleBuilder</name>
    </buildCommand>
    <buildCommand>
      <name>org.mule.tooling.maven.mavenBuilder</name>
    </buildCommand>
  </buildSpec>
  <natures>
    <nature>org.mule.tooling.maven.mavenNature</nature>
    <nature>org.mule.tooling.core.muleDomainNature</nature>
    <nature>org.mule.tooling.core.muleNature</nature>
    <nature>org.eclipse.jdt.core.javanature</nature>
  </natures>
</projectDescription>